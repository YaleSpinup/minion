{ 
  "accounts": {
    "myaccount": {
      "runners": ["dummyRunner", "instanceRunner"]
    }
  },
  "jobRunners": {
    "dummyRunner": {
      "type": "dummy",
      "config": {
        "template": "Hello, {{.Account}}!"
      }
    },
    "instanceRunner": {
      "type": "instance",
      "config": {
        "endpointTemplate": "http://127.0.0.1:8080/v1/ec2/{{.Account}}/instances/{{.InstanceID}}/power",
        "token": "yyyyyyyy"
      }
    }
  },
  "jobsRepository": {
    "type": "s3",
    "refreshInterval": "60m",
    "config": {
      "region": "us-east-1",
      "akid": "keykeykeykeykeykeykey",
      "secret": "secretsecretsecretsecretsecret",
      "bucket": "myjobsrepository",
      "prefix": "jobs"
    }
  },
  "queueProvider": {
    "type": "redis",
    "config": {
      "host": "127.0.0.1",
      "port": 6379,
      "database": 2
    }
  },
  "lockProvider": {
    "type": "redis",
    "config": {
      "host": "127.0.0.1",
      "port": 6379,
      "database": 2
    }
  },
  "listenAddress": ":8080",
  "token": "xxxxxx",
  "logLevel": "info",
  "org": "localdev"
}